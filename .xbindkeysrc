# Increase volume
"pamixer -i 10"
   XF86AudioRaiseVolume

# Decrease volume
"pamixer -d 10"
   XF86AudioLowerVolume

# Mute volume
"pamixer -t"
   XF86AudioMute

# Mute mic
# "pamixer --list-sources|grep input| awk '{print $1}' | xargs -I '{}' pamixer --source '{}' -t"
"pamixer --default-source -t"
   XF86AudioMicMute

# Increase backlight
"light -A 10"
   XF86MonBrightnessUp

# Decrease backlight
"light -U 10"
   XF86MonBrightnessDown

